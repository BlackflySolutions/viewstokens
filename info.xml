<?xml version="1.0"?>
<extension key="viewstokens" type="module">
  <file>viewstokens</file>
  <name>Views Tokens</name>
  <description>CiviCRM Tokens generated from Drupal Views</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alan Dixon</author>
    <email>info@blackflysolutions.ca</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.org/blackflysolutions/viewstokens</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-03-09</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
